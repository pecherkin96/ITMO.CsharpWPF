<Window x:Class="ITMO.CsharpWPF.Lab2._2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.CsharpWPF.Lab2._2"
        mc:Ignorable="d"
        Title="MainWindow" Height="288" Width="566">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="431*"/>
            <ColumnDefinition Width="361*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="txtBox" HorizontalAlignment="Left" Height="46" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="10,48,0,0"/>
        <StackPanel ButtonBase.Click="StackPanel_Click"  HorizontalAlignment="Left" Height="46" VerticalAlignment="Top" Width="100" Margin="162,48,0,0">
            <Button x:Name="butAdd" Content="x+x"/>
            <Button x:Name="butMult" Content="x*x"/>
        </StackPanel>

    </Grid>
</Window>
