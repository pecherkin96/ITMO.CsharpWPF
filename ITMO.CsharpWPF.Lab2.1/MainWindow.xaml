<Window x:Class="ITMO.CsharpWPF.Lab2._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.CsharpWPF.Lab2._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        TextBoxBase.TextChanged="Window_TextChanged">
    <Grid TextBoxBase.TextChanged="Grid_TextChanged">
        <RadioButton x:Name="radBut3" Content="Обработка события в Window" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="29,118,0,0"/>
        <RadioButton x:Name="radBut2" Content="Обработка события в Grid " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="29,75,0,0"/>
        <RadioButton x:Name="radBut1" Content="Обработка события в Textbox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="29,38,0,0"/>
        <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="197" Margin="263,67,0,0" TextChanged="TextBox_TextChanged"/>
        
    </Grid>
</Window>
